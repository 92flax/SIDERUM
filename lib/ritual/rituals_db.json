    "intention": "Power",
    "traditionTag": "Golden Dawn",
    "steps": [
      {
        "order": 1,
        "action_type": "GESTURE",
        "instruction_text": "Perform the Qabalistic Cross. Touch your forehead.",
        "audio_vibration": { "word": "Ateh", "phonetic": "Ah-teh" }
      },
      {
        "order": 2,
        "action_type": "GESTURE",
        "instruction_text": "Touch your chest, pointing downward.",
        "audio_vibration": { "word": "Malkuth", "phonetic": "Mal-kooth" }
      },
      {
        "order": 3,
        "action_type": "GESTURE",
        "instruction_text": "Touch your right shoulder, then left shoulder.",
        "audio_vibration": { "word": "Ve-Geburah, Ve-Gedulah", "phonetic": "Veh-Geh-boo-rah, Veh-Geh-doo-lah" }
      },
      {
        "order": 4,
        "action_type": "MOVEMENT",
        "instruction_text": "Face East. Prepare to trace the Invoking Air Pentagram.",
        "compass_direction": "EAST"
      },
      {
        "order": 5,
        "action_type": "TRACE",
        "instruction_text": "Trace the Invoking Pentagram of Air. Vibrate the name of the element.",
