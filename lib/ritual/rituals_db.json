        "ar_element": { "shape": "Pentagram", "color_hex": "#FFFF00", "position": "EAST" }
      },
      {
        "order": 6,
        "action_type": "VIBRATION",
        "instruction_text": "Vibrate the Divine Name for Air.",
        "compass_direction": "EAST",
        "audio_vibration": { "word": "YHVH", "phonetic": "Yod-Heh-Vav-Heh" }
      },
      {
        "order": 7,
        "action_type": "MOVEMENT",
        "instruction_text": "Turn to face the South. Trace a line of light.",
        "compass_direction": "SOUTH"
      },
      {
        "order": 8,
        "action_type": "TRACE",
        "instruction_text": "Trace the Invoking Pentagram of Fire.",
        "compass_direction": "SOUTH",
        "ar_element": { "shape": "Pentagram", "color_hex": "#EF4444", "position": "SOUTH" }
      },
      {
        "order": 9,
        "action_type": "VIBRATION",
        "instruction_text": "Vibrate the Divine Name for Fire.",
        "compass_direction": "SOUTH",
        "audio_vibration": { "word": "ALHIM", "phonetic": "El-oh-heem" }
      },
      {
        "order": 10,
        "action_type": "MOVEMENT",
        "instruction_text": "Turn to face the West.",
        "compass_direction": "WEST"
      },
      {
        "order": 11,
        "action_type": "TRACE",
        "instruction_text": "Trace the Invoking Pentagram of Water.",
        "compass_direction": "WEST",
        "ar_element": { "shape": "Pentagram", "color_hex": "#3B82F6", "position": "WEST" }
      },
      {
        "order": 12,
        "action_type": "VIBRATION",
        "instruction_text": "Vibrate the Divine Name for Water.",
        "compass_direction": "WEST",
        "audio_vibration": { "word": "AL", "phonetic": "El" }
      },
      {
        "order": 13,
        "action_type": "MOVEMENT",
        "instruction_text": "Turn to face the North.",
        "compass_direction": "NORTH"
      },
      {
        "order": 14,
        "action_type": "TRACE",
        "instruction_text": "Trace the Invoking Pentagram of Earth.",
        "compass_direction": "NORTH",
        "ar_element": { "shape": "Pentagram", "color_hex": "#22C55E", "position": "NORTH" }
      },
      {
        "order": 15,
        "action_type": "VIBRATION",
        "instruction_text": "Vibrate the Divine Name for Earth.",
        "compass_direction": "NORTH",
        "audio_vibration": { "word": "AGLA", "phonetic": "Ah-geh-lah" }
      },
      {
        "order": 16,
        "action_type": "GESTURE",
        "instruction_text": "Return to the East. Close with the Qabalistic Cross. The ritual is complete.",
        "compass_direction": "EAST",
        "audio_vibration": { "word": "Le-Olam, Amen", "phonetic": "Leh-Oh-lahm, Ah-men" }
      }
    ]
  },
  {
    "id": "star_ruby",
    "name": "The Star Ruby",
    "description": "Aleister Crowley's Thelemic adaptation of the LBRP. A powerful banishing ritual using Greek god-names and the Sign of Harpocrates.",
    "tradition": "Thelema",
    "intention": "Purification",
    "traditionTag": "Thelema",
    "steps": [
      {
        "order": 1,
        "action_type": "GESTURE",
        "instruction_text": "Stand in the center. Touch your forehead with your right hand.",
        "audio_vibration": { "word": "SOI", "phonetic": "Soy" }
      },
      {
        "order": 2,
        "action_type": "GESTURE",
        "instruction_text": "Touch your groin area.",
        "audio_vibration": { "word": "O PHALLE", "phonetic": "Oh Fal-leh" }
      },
      {
        "order": 3,
        "action_type": "GESTURE",
        "instruction_text": "Touch your right shoulder.",
        "audio_vibration": { "word": "ISCHUROS", "phonetic": "Is-koo-ros" }
      },
      {
        "order": 4,
        "action_type": "GESTURE",
        "instruction_text": "Touch your left shoulder.",
        "audio_vibration": { "word": "EUCHARISTOS", "phonetic": "Yoo-ka-ris-tos" }
      },
      {
        "order": 5,
        "action_type": "GESTURE",
        "instruction_text": "Clasp your hands upon your breast.",
        "audio_vibration": { "word": "IAO", "phonetic": "Ee-Ah-Oh" }
      },
      {
        "order": 6,
        "action_type": "MOVEMENT",
        "instruction_text": "Turn to face the East.",
        "compass_direction": "EAST"
      },
      {
        "order": 7,
        "action_type": "TRACE",
        "instruction_text": "Trace a banishing pentagram in the East.",
        "compass_direction": "EAST",
        "ar_element": { "shape": "Pentagram", "color_hex": "#FFD700", "position": "EAST" }
      },
      {
        "order": 8,
        "action_type": "VIBRATION",
        "instruction_text": "Charge the pentagram with the name.",
        "compass_direction": "EAST",
        "audio_vibration": { "word": "THERION", "phonetic": "Theh-ree-on" }
      },
      {
        "order": 9,
        "action_type": "MOVEMENT",
        "instruction_text": "Turn to face the South.",
        "compass_direction": "SOUTH"
      },
      {
        "order": 10,
        "action_type": "TRACE",
        "instruction_text": "Trace a banishing pentagram in the South.",
        "compass_direction": "SOUTH",
        "ar_element": { "shape": "Pentagram", "color_hex": "#EF4444", "position": "SOUTH" }
      },
      {
        "order": 11,
        "action_type": "VIBRATION",
        "instruction_text": "Charge the pentagram.",
        "compass_direction": "SOUTH",
        "audio_vibration": { "word": "NUIT", "phonetic": "Noo-eet" }
      },
      {
        "order": 12,
        "action_type": "MOVEMENT",
        "instruction_text": "Turn to face the West.",
        "compass_direction": "WEST"
      },
      {
        "order": 13,
        "action_type": "TRACE",
        "instruction_text": "Trace a banishing pentagram in the West.",
        "compass_direction": "WEST",
        "ar_element": { "shape": "Pentagram", "color_hex": "#3B82F6", "position": "WEST" }
      },
      {
        "order": 14,
        "action_type": "VIBRATION",
        "instruction_text": "Charge the pentagram.",
        "compass_direction": "WEST",
        "audio_vibration": { "word": "BABALON", "phonetic": "Bab-ah-lon" }
      },
      {
        "order": 15,
        "action_type": "MOVEMENT",
        "instruction_text": "Turn to face the North.",
        "compass_direction": "NORTH"
      },
      {
        "order": 16,
        "action_type": "TRACE",
        "instruction_text": "Trace a banishing pentagram in the North.",
        "compass_direction": "NORTH",
        "ar_element": { "shape": "Pentagram", "color_hex": "#22C55E", "position": "NORTH" }
      },
      {
        "order": 17,
        "action_type": "VIBRATION",
        "instruction_text": "Charge the pentagram.",
        "compass_direction": "NORTH",
        "audio_vibration": { "word": "HADIT", "phonetic": "Ha-deet" }
      },
      {
        "order": 18,
        "action_type": "VISUALIZATION",
        "instruction_text": "Extend your arms in the form of Tau. Visualize the four pentagrams blazing at the quarters. Say: Pro Mou Iunges, Opiso Mou Teletarchai, Epi Dexia Sunoches, Ep Aristera Daimones."
      },
      {
        "order": 19,
        "action_type": "GESTURE",
        "instruction_text": "Make the Sign of Harpocrates (finger to lips). The ritual is complete.",
        "audio_vibration": { "word": "ABRAHADABRA", "phonetic": "Ab-ra-ha-dab-ra" }
      }
    ]
  },
  {
    "id": "hammer_rite",
    "name": "The Hammer Rite",
    "description": "A Norse-inspired ritual for consecrating and protecting a space using the power of Thor's hammer Mjölnir.",
    "tradition": "Norse",
    "intention": "Protection",
    "traditionTag": "Norse",
    "steps": [
      {
        "order": 1,
        "action_type": "GESTURE",
        "instruction_text": "Stand in the center of your space. Close your eyes and feel the roots of Yggdrasil beneath your feet."
      },
      {
        "order": 2,
        "action_type": "MOVEMENT",
        "instruction_text": "Face North. Raise your right hand high, then bring it down forcefully as if striking with a hammer.",
        "compass_direction": "NORTH"
      },
      {
        "order": 3,
        "action_type": "VIBRATION",
        "instruction_text": "As you strike, call upon the Thunderer.",
        "compass_direction": "NORTH",
        "audio_vibration": { "word": "THUNOR", "phonetic": "Thoo-nor" }
      },
      {
        "order": 4,
        "action_type": "TRACE",
        "instruction_text": "Trace the Hammer sign (an inverted T) in the air before you to the North.",
        "compass_direction": "NORTH",
        "ar_element": { "shape": "Cross", "color_hex": "#A0A0A0", "position": "NORTH" }
      },
      {
        "order": 5,
        "action_type": "MOVEMENT",
        "instruction_text": "Turn to face the East.",
        "compass_direction": "EAST"
      },
      {
        "order": 6,
        "action_type": "VIBRATION",
        "instruction_text": "Strike the hammer sign to the East.",
        "compass_direction": "EAST",
        "audio_vibration": { "word": "THUNOR", "phonetic": "Thoo-nor" }
      },
      {
        "order": 7,
        "action_type": "TRACE",
        "instruction_text": "Trace the Hammer sign to the East.",
        "compass_direction": "EAST",
        "ar_element": { "shape": "Cross", "color_hex": "#A0A0A0", "position": "EAST" }
      },
      {
        "order": 8,
        "action_type": "MOVEMENT",
        "instruction_text": "Turn to face the South.",
        "compass_direction": "SOUTH"
      },
      {
        "order": 9,
        "action_type": "VIBRATION",
        "instruction_text": "Strike the hammer sign to the South.",
        "compass_direction": "SOUTH",
        "audio_vibration": { "word": "THUNOR", "phonetic": "Thoo-nor" }
      },
      {
        "order": 10,
        "action_type": "TRACE",
        "instruction_text": "Trace the Hammer sign to the South.",
        "compass_direction": "SOUTH",
        "ar_element": { "shape": "Cross", "color_hex": "#A0A0A0", "position": "SOUTH" }
      },
      {
        "order": 11,
        "action_type": "MOVEMENT",
        "instruction_text": "Turn to face the West.",
        "compass_direction": "WEST"
      },
      {
        "order": 12,
        "action_type": "VIBRATION",
        "instruction_text": "Strike the hammer sign to the West.",
        "compass_direction": "WEST",
        "audio_vibration": { "word": "THUNOR", "phonetic": "Thoo-nor" }
      },
      {
        "order": 13,
        "action_type": "TRACE",
        "instruction_text": "Trace the Hammer sign to the West.",
        "compass_direction": "WEST",
        "ar_element": { "shape": "Cross", "color_hex": "#A0A0A0", "position": "WEST" }
      },
      {
        "order": 14,
        "action_type": "VISUALIZATION",
        "instruction_text": "Return to the North. Visualize four glowing Hammer signs at the quarters, connected by a ring of lightning. Your space is consecrated. Hail Thor!"
      }
    ]
  }
]
